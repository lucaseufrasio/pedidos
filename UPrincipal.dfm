  object FDUpdPedido: TFDUpdateSQL
    Connection = fdConexao
    InsertSQL.Strings = (
      'INSERT INTO wk.pedidos'
      '(codigo, data, clientes_codigo, total)'
      
        'VALUES (:NEW_codigo, :NEW_data, :NEW_clientes_codigo, :NEW_total' +
        ')')
    ModifySQL.Strings = (
      'UPDATE wk.pedidos'
      
        'SET Id = :NEW_Id, codigo = :NEW_codigo, data = :NEW_data, client' +
        'es_codigo = :NEW_clientes_codigo, '
      '  total = :NEW_total'
      'WHERE Id = :OLD_Id')
    DeleteSQL.Strings = (
      'DELETE FROM wk.pedidos'
      'WHERE Id = :OLD_Id')
    FetchRowSQL.Strings = (
      
        'SELECT LAST_INSERT_ID() AS Id, codigo, data, clientes_codigo, to' +
